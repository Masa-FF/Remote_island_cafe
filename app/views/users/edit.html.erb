<article class="menu-base">
  <div class="base-title">
    <h2>EDIT PROFILE</h2>
    <p>会員情報を編集</p></p>
  </div>
</article>

<div class="section">
  <%= form_with model:@user, local:true do |f| %>
  <label>ニックネーム</label>
  <%= f.text_field :name, autofocus: true, placeholder:"名前" %>  
  <br>
  <label>メールアドレス</label>
  <%= f.text_field :email, autofocus: true, placeholder:"メールアドレス" %>
  <br>  
  <%= f.submit "変更を保存" %>
  <% end %>
</div>